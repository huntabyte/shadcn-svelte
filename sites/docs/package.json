		"typescript": "~5.5.3",
		"unified": "^11.0.5",
		"unist-builder": "^3.0.1",
		"unist-util-visit": "^5.0.0",
		"vaul-svelte": "^1.0.0-next.2",
		"velite": "^0.2.0",
