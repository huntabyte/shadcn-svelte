{
    "useTabs": true,
    "singleQuote": false,
    "trailingComma": "none",
    "printWidth": 100,
    "plugins": [
        "prettier-plugin-svelte"
    ],
    "pluginSearchDirs": false,
    "overrides": [
        {
            "files": "*.svelte",
            "options": {
                "parser": "svelte",
                "svelteIndentScriptAndStyle": true,
                "svelteStrictMode": false,
                "svelteSortOrder": "scripts-markup-styles-options"
            }
        }
    ],
    "bracketSameLine": false
}